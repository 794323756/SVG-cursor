{
  "name": "bitmap-to-svg",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy:vercel": "vercel deploy --prod",
    "deploy:docker": "docker build -t svg-cursor . && docker push svg-cursor",
    "deploy:github": "gh-pages -d dist",
    "test": "vitest",
    "test:e2e": "cypress run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@vectormage/image-tracer": "^1.0.0",
    "comlink": "^4.4.1",
    "d3-color": "^3.1.0",
    "file-saver": "^2.0.5",
    "opencv.js": "^1.2.1",
    "potrace": "^2.1.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3"
  },
  "devDependencies": {
    "@types/d3-color": "^3.0.7",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.11.19",
    "@types/potrace": "^2.1.4",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "cypress": "^13.6.4",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vitest": "^1.2.2"
  }
} 